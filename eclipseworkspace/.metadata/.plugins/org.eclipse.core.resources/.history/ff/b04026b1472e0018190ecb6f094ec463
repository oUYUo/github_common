package com.method;

public class Demo1_Name {
	public static void main(String[] args) {
		new Thread("A"){
			public void run() {
				System.out.println(this.getName()+"aaaaaaaaaaaaa");
				
			}
		}.start();
		
		new Thread("B"){
			public void run() {
				System.out.println(this.getName()+"bbbbbbbbbbbbbb");
				
			}
		}.start();
	}
}
